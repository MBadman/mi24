	class Components
	{
		class SensorsManagerComponent
		{
			class Components
			{
				class VisualSensorComponent
				{
					class AirTarget
					{
						minRange = 500;
						maxRange = 2000;
						objectDistanceLimitCoef = -1;
						viewDistanceLimitCoef = 1;
					};
					class GroundTarget
					{
						minRange = 500;
						maxRange = 3200;
						objectDistanceLimitCoef = 1;
						viewDistanceLimitCoef = 1;
					};
